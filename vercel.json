{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload",
      "methods": ["POST"]
    },
    {
      "src": "/file/(.*)",
      "dest": "/file/$1",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET"]
    }
  ]
}